{
  "expo": {
    "name": "BookWorm",
    "scheme": "bookworm",
    "slug": "Demo1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/play_store_512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash_latest.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bookworm.demo"
    },
    "experiments": {
      "reactCompiler": true
    },
    "android": {
      "package": "com.bookworm.demo",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./src/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon_latest.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "bookworm.infodata.in"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/play_store_512.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.20"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "685ddbfc-a7b9-4ad0-9268-61d134a63996"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/685ddbfc-a7b9-4ad0-9268-61d134a63996"
    }
  }
}
